{
  "name": "node-etcd-7",
  "version": "5.0.3",
  "description": "etcd library for node.js (etcd v2 api)",
  "licenses": [
    {
      "type": "BSD 3-Clause",
      "url": "http://opensource.org/licenses/BSD-3-Clause"
    }
  ],
  "maintainers": [
    {
      "name": "Stian Eikeland",
      "email": "stian@eikeland.se",
      "web": "http://eikeland.se/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/stianeikeland/node-etcd.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "node_modules/.bin/coffee --bare --compile --output lib/ src",
    "prepublish": "node_modules/.bin/coffee --bare --compile --output lib/ src",
    "postpublish": "rm -rf lib",
    "test": "node_modules/.bin/mocha --compilers coffee:coffee-script/register test/",
    "watch": "node_modules/.bin/mocha --compilers coffee:coffee-script/register --watch"
  },
  "dependencies": {
    "deasync": "0.1.9",
    "request": "2.60.0",
    "underscore": "1.8.2",
    "url-parse": "1.0.5"
  },
  "devDependencies": {
    "coffee-script": "1.9.1",
    "mocha": "2.1.0",
    "nock": "2.17.0",
    "should": "5.0.1",
    "simple-mock": "0.5.0"
  },
  "keywords": [
    "etcd",
    "raft"
  ]
}
